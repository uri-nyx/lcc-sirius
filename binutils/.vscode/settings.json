{
    "C_Cpp.default.compilerPath": "/usr/bin/clang",
    "c-cpp-flylint.clang.includePaths": [
        "${workspaceFolder}/include",
    ],
    "c-cpp-flylint.clang.pedantic": true,
    "c-cpp-flylint.clang.standard": [
        "c89"
    ],
    "c-cpp-flylint.language": "c",
    "c-cpp-flylint.standard": [
        "c89",
    ],
    "c-cpp-flylint.cppcheck.enable": false,
    "c-cpp-flylint.flawfinder.enable": false,
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.lizard.enable": false,
    "C_Cpp.dimInactiveRegions": true,
    "C_Cpp.default.cStandard": "c89",
    "C_Cpp.inlayHints.parameterNames.enabled": true,
    "C_Cpp.clang_format_fallbackStyle": "none",
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.clang_format_path": "",
    "editor.tabSize": 8,
    "editor.formatOnSave": true,
    "editor.fontFamily": "'Berkeley Mono', monospace",
    "debug.console.fontFamily": "'Berkeley Mono'",
    "scm.inputFontFamily": "editor",
    "editor.codeLensFontFamily": "'Berkeley Mono'",
    "C/C++ Include Guard.Macro Type": "Filename",
    "files.eol": "\n",
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "files.associations": {
        "*.rasi": "css",
        "*.bbmodel": "json",
        "*.sublime-snippet": "xml",
        "*.hbs": "html",
        "*.ejs": "html",
        "*.emu": "html",
        "lesskey": "lesskey",
        "*.Xresources": "xdefaults",
        "i3/config": "i3",
        "i3/*.conf": "i3",
        "polybar/config": "ini",
        "polybar/*.conf": "ini",
        "*.s": "riscv",
        "*.asm": "riscv",
        "*.h": "c",
        "random": "c",
        "cstdlib": "c",
        "*.tcc": "c"
    },
    "editor.rulers": [
        80
    ],
    "justify.defaultLineWidth": 80,
    "[markdown]": {
        "editor.rulers": [60],
        "justify.defaultLineWidth": 60,
    },
}
